{
  "name": "ad-proj",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /ad-proj/ && npm run copy-docs",
    "copy-docs:win": "xcopy dist\\ad-proj\\browser docs /E /I /Y",
    "copy-docs:unix": "cp -r dist/ad-proj/browser/* docs/",
    "copy-docs": "npm run clean-docs && (npm run copy-docs:win || npm run copy-docs:unix)",
    "clean-docs:win": "if exist docs rmdir /s /q docs",
    "clean-docs:unix": "rm -rf docs",
    "clean-docs": "npm run clean-docs:win || npm run clean-docs:unix",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ad-proj": "node dist/ad-proj/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.2",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.2.2",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.2.4",
    "@angular/ssr": "^20.1.6",
    "express": "^5.1.0",
    "firebase": "^11.10.0",
    "marked": "^16.2.1",
    "ngx-markdown": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2"
  }
}
